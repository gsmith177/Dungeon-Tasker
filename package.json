{
  "name": "dungeon-tasker",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently --names \"vite,electron\" --prefix-colors \"blue,green\" \"npm run web:dev\" \"npm run electron:dev\"",
    "web:dev": "cd frontend/web && npm run dev",
    "electron:dev": "cd frontend/electron && npm run start"
  },
  "devDependencies": {
    "concurrently": "^9.1.1"
  },
  "dependencies": {
    "@types/react-grid-layout": "^1.3.6",
    "react-grid-layout": "^2.2.2"
  }
}
